<odoo>
    <record id="planning_ce.planning_compare_wizard" model="ir.ui.view">
        <field name="name">ce_planner.report.wizard.form</field>
        <field name="model">ce_planner.report.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Compare planned vs spent">
                <group>
                    <field name="date_start"/>
                    <field name="date_end"/>
                </group>
                <footer>
                    <button name="create_reports" string="Create" type="object" class="btn-primary"/>
                    <button name="cancel" string="Cancel" special="cancel" class="oe_link"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
